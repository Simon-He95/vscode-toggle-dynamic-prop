{
  "publisher": "simonhe",
  "name": "vscode-toggle-dynamic-prop",
  "displayName": "vscode-toggle-dynamic-prop",
  "version": "0.0.49",
  "description": "vscode-toggle-dynamic-prop",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/vscode-toggle-dynamic-prop#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/vscode-toggle-dynamic-prop"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vscode-toggle-dynamic-prop/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-toggle-dynamic-prop.toggleDynamicProp",
        "title": "%vscode-toggle-dynamic-prop.title%"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-toggle-dynamic-prop.toggleDynamicProp",
        "key": "cmd+t",
        "when": "isMac"
      },
      {
        "command": "vscode-toggle-dynamic-prop.toggleDynamicProp",
        "key": "ctrl+t",
        "when": "isWindows"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@types/node": "^18.19.80",
    "@types/vscode": "1.75.1",
    "@vscode-use/utils": "0.1.39",
    "@vscode/vsce": "^3.3.0",
    "bumpp": "^9.11.1",
    "eslint": "^9.22.0",
    "lazy-js-utils": "^0.1.33",
    "tsup": "^6.7.0",
    "typescript": "^5.8.2",
    "vitest": "^0.29.8"
  }
}
